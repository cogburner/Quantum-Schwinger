Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{
  "\[Chi]", ",", "\[Chi]d", ",", "temp\[Chi]d", ",", "adj", ",", "adjzeros", 
   ",", "toexpr", ",", "table", ",", "tempans"}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Chi]d", "[", 
   RowBox[{"N0_", ",", "n0_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"N", "=", "N0"}], ",", 
      RowBox[{"n", "=", "n0"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"temp\[Chi]d", "=", 
      RowBox[{"ArrayFlatten", "[", 
       RowBox[{"ArrayFlatten", "[", 
        RowBox[{
         RowBox[{"ArrayFlatten", "[", 
          RowBox[{"TensorProduct", "[", 
           RowBox[{
            RowBox[{"IdentityMatrix", "[", 
             SuperscriptBox["2", 
              RowBox[{"n", "-", "1"}]], "]"}], ",", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"0", ",", "0"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"1", ",", "0"}], "}"}]}], "}"}], ",", " ", 
            RowBox[{"IdentityMatrix", "[", 
             SuperscriptBox["2", 
              RowBox[{"N", "-", "n"}]], "]"}]}], "]"}], "]"}], ",", "2"}], 
        "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"adj", "=", 
      RowBox[{
       RowBox[{"SparseArray", "[", "temp\[Chi]d", "]"}], "[", 
       "\"\<AdjacencyLists\>\"", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"adjzeros", "=", 
      RowBox[{"adj", "/.", 
       RowBox[{
        RowBox[{"{", "}"}], "->", "0"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"toexpr", "=", 
      RowBox[{
       RowBox[{"ToExpression", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"Characters", "@", 
           RowBox[{
            RowBox[{"IntegerString", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"adjzeros", "[", 
                RowBox[{"[", "i", "]"}], "]"}], "-", "1"}], ",", "2", ",", 
              "N"}], "]"}], "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", 
            SuperscriptBox["2", "N"]}], "}"}]}], "]"}], "]"}], "//", 
       "Quiet"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"table", "=", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"toexpr", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "[", 
            RowBox[{"[", "j", "]"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"j", ",", "1", ",", "n"}], "}"}]}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", 
           SuperscriptBox["2", "N"]}], "}"}]}], "]"}], "//", "Quiet"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"tempans", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"(*", "\[ImaginaryI]", "**)"}], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], 
         RowBox[{"table", "[", 
          RowBox[{"[", "i", "]"}], "]"}]], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          SuperscriptBox["2", "N"]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"temp\[Chi]d", "[", 
         RowBox[{"[", 
          RowBox[{"i", ",", "j"}], "]"}], "]"}], "*", 
        RowBox[{"tempans", "[", 
         RowBox[{"[", "i", "]"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", 
         SuperscriptBox["2", "N"]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"j", ",", "1", ",", 
         SuperscriptBox["2", "N"]}], "}"}]}], "]"}]}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Chi]", "[", 
   RowBox[{"N0_", ",", "n0_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"\[Chi]", "[", 
    RowBox[{"N0", ",", "n0"}], "]"}], "=", 
