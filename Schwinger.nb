Cell[TextData[{
 "Recall that when dealing with tensor products, the operator ",
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{
     SubscriptBox["\[Chi]", "n"], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"1", "\[TensorProduct]"}], "..."}], "\[TensorProduct]", 
        "\[Chi]", "\[TensorProduct]", "1", "\[TensorProduct]"}], "..."}], 
      "\[TensorProduct]", "1"}]}],
    FontSize->24], TraditionalForm]],ExpressionUUID->
  "70c7b520-5568-4d05-886e-0f227e6b2242"],
 " where ",
 Cell[BoxData[
  FormBox["\[Chi]", TraditionalForm]],ExpressionUUID->
  "1790e93b-af39-435e-8090-476af653385b"],
 " is in the ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "6fa6430b-e559-4641-a243-2bbc661cfbaa"],
 "-th position. Hence, the fermion creation/annihilation operators are \
constructed through"
}], "Text",
 CellChangeTimes->{{3.931340781714579*^9, 3.931340842760556*^9}},
 Background->RGBColor[
  1, 0.9, 0.8],ExpressionUUID->"2d70ea01-9be9-499a-ab45-220f03e5f47e"],

Cell[TextData[{
 "The fermionic commutation relations are given by",
 StyleBox[" ",
  FontSize->24],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["\[Chi]", "i"], ",", 
      SubscriptBox["\[Chi]", "j"]}], "}"}], "=", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       SubscriptBox[
        SuperscriptBox["\[Chi]", "\[Dagger]"], "i"], ",", 
       SubscriptBox[
        SuperscriptBox["\[Chi]", "\[Dagger]"], "j"]}], "}"}], "=", "0"}]}], 
   TraditionalForm]],
  FontSize->24,ExpressionUUID->"06e6c850-f58a-4208-8d5f-7692afe59621"],
 StyleBox[", ",
  FontSize->24],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["\[Chi]", "i"], ",", 
      SubscriptBox[
       SuperscriptBox["\[Chi]", "\[Dagger]"], "j"]}], "}"}], "=", 
    SubscriptBox["\[Delta]", "ij"]}], TraditionalForm]],
  FontSize->24,ExpressionUUID->"9cd48a71-da6f-4b8f-a47c-f8fa613e0002"],
 ", or\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      SuperscriptBox["\[Chi]", "\[Dagger]"], "i"], "|", 
     SubscriptBox["n", "1"]}], ",", "...", ",", 
    SubscriptBox["n", "i"], ",", "...", ",", 
    RowBox[{
     SubscriptBox["n", "N"], ">=", 
     RowBox[{"\[Piecewise]", 
      TagBox[GridBox[{
         {
          RowBox[{
           RowBox[{
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{"-", "1"}], ")"}], 
             RowBox[{
              SubscriptBox["\[Sum]", 
               RowBox[{"j", "<", "i"}]], 
              SubscriptBox["n", "j"]}]], "|", 
            SubscriptBox["n", "1"]}], ",", "...", ",", 
           RowBox[{
            SubscriptBox["n", "i"], "+", "1"}], ",", "...", ",", 
           RowBox[{
            SubscriptBox["n", "N"], ">"}], ",", "       ", 
           RowBox[{
            SubscriptBox["n", "i"], "=", "0"}]}]},
         {
          RowBox[{
          "0", " ", ",", 
           "                                                                  \
               ", 
           RowBox[{
            SubscriptBox["n", "i"], "=", "1"}]}]}
        },
        AutoDelete->False,
        GridBoxAlignment->{"Columns" -> {{Left}}},
        GridBoxItemSize->{
         "Columns" -> {{Automatic}}, "Rows" -> {{Automatic}}}],
       "Grid"]}]}]}], TraditionalForm]],
  FontSize->24,ExpressionUUID->"0c3b9e40-03e6-47b6-b012-138aa257fc9b"],
 StyleBox["  ;      ",
  FontSize->24],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Chi]", "i"], "|", 
     SubscriptBox["n", "1"]}], ",", "...", ",", 
    SubscriptBox["n", "i"], ",", "...", ",", 
    RowBox[{
     SubscriptBox["n", "N"], ">=", 
     RowBox[{"\[Piecewise]", 
      TagBox[GridBox[{
         {
          RowBox[{
           RowBox[{
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{"-", "1"}], ")"}], 
             RowBox[{
              SubscriptBox["\[Sum]", 
               RowBox[{"j", "<", "i"}]], 
              SubscriptBox["n", "j"]}]], "|", 
            SubscriptBox["n", "1"]}], ",", "...", ",", 
           RowBox[{
            SubscriptBox["n", "i"], "-", "1"}], ",", "...", ",", 
           RowBox[{
            SubscriptBox["n", "N"], ">"}], ",", "      ", 
           RowBox[{
            SubscriptBox["n", "i"], "=", "0"}]}]},
         {
          RowBox[{
          "0", ",", 
           "                                                                  \
               ", 
           RowBox[{
            SubscriptBox["n", "i"], "=", "1"}]}]}
        },
        AutoDelete->False,
        GridBoxAlignment->{"Columns" -> {{Left}}},
        GridBoxItemSize->{
         "Columns" -> {{Automatic}}, "Rows" -> {{Automatic}}}],
       "Grid"]}]}]}], TraditionalForm]],
  FontSize->24,ExpressionUUID->"37bf84e7-2c9c-4e92-95bd-18de6acb1c9b"],
 ".\n",
 StyleBox["We need to ensure that this algebra is obeyed.",
  FontSize->14]
}], "Text",
