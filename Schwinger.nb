Cell[TextData[{
 "The Jordan-Wigner transformation maps spin operators (which let us put it \
on the quantum hardware easily) and fermionic creation/annihilation operators \
via the transformation\n\t\t\t\t\t\t\t\t\t\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      SuperscriptBox["\[Chi]", "\[Dagger]"], "j"], "=", 
     RowBox[{
      UnderscriptBox["\[Product]", 
       RowBox[{"l", "<", "j"}]], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"\[ImaginaryI]", " ", 
         SubscriptBox[
          SuperscriptBox["\[Sigma]", "z"], "l"]}], ")"}], 
       SubscriptBox[
        SuperscriptBox["\[Sigma]", "-"], "j"]}]}]}], ",", "     ", 
    RowBox[{
     SubscriptBox["\[Chi]", "j"], "=", 
     RowBox[{
      UnderscriptBox["\[Product]", 
       RowBox[{"l", "<", "j"}]], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "\[ImaginaryI]"}], " ", 
         SubscriptBox[
          SuperscriptBox["\[Sigma]", "z"], "l"]}], ")"}], 
       RowBox[{
        SubscriptBox[
         SuperscriptBox["\[Sigma]", "+"], "j"], ".", "  "}]}]}]}]}], 
   TraditionalForm]],
  FontSize->24,ExpressionUUID->"67dfb28e-f3b0-4667-9d15-19b1ed3f34b5"]
}], "Text",
